{    
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "qmiCommon": {
            "type": "object",
            "metadata": {
                "description": "Networking Parameters passed as an object"
            }
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.KeyVault/vaults",
            "apiVersion": "2016-10-01",
            "location": "[parameters('qmiCommon').location]",
            "name": "[parameters('qmiCommon').keyVaultName]",
            "properties": {
                "enableSoftDelete": true ,
                "tenantId": "[subscription().tenantId]",
                "sku": {
                    "family": "A",
                    "name": "standard"
                
                }
            }
        }
    ],
    "outputs": {
        "vaultUri": {
            "type": "string",
            "value": "[reference(resourceId('Microsoft.KeyVault/vaults/',parameters('qmiCommon').keyVaultName ),'Full').properties.vaultUri]"
        }
    }
}